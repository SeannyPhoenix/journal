{
  "name": "bogie",
	"image": "mcr.microsoft.com/devcontainers/base:debian",
  "features": {
		"ghcr.io/devcontainers/features/aws-cli:1": "latest",
		"ghcr.io/devcontainers/features/docker-in-docker:2": {},
		"ghcr.io/devcontainers/features/github-cli": "latest",
		"ghcr.io/devcontainers/features/go": {},
		"ghcr.io/devcontainers-contrib/features/apt-packages": {"packages": "bash-completion"},
		"ghcr.io/devcontainers-contrib/features/typescript": {}
	},
	"containerEnv": {
		"AWS_PROFILE": "bogie"
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"Github.copilot",
				"GitHub.copilot-chat"
			]
		}
	}
}
